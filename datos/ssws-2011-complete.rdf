  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/ssws2011/data.semanticweb.org/workshop/ssws/2011/paper/10">
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/mark-a-musen"/>
    <cofunc:isPartOf rdf:resource="https://w3id.org/scholarlydata/workshop/ssws/2011/proceedings"/>

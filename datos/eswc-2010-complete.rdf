    <cofunc:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/eswc2010/paper/web_of_data/8/authorList"/>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/wei-hu"/>
  </rdf:Description>

    <cofunc:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/eswc2010/paper/mobility/6/authorList"/>
    <cofunc:title>Replication and Versioning of Partial RDF Graphs</cofunc:title>

  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/owled2007/paper-7">
    <cofunc:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/owled2007/paper-7/authorList"/>

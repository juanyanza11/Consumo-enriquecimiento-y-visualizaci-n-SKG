    <cofunc:isPartOf rdf:resource="https://w3id.org/scholarlydata/conference/iswc/2010/proceedings"/>
    <cofunc:title>Linked Data Query Processing Strategies</cofunc:title>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/thanh-tran"/>
    <cofunc:keyword>semantic web</cofunc:keyword>
  </rdf:Description>

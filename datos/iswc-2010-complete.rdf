    <cofunc:isPartOf rdf:resource="https://w3id.org/scholarlydata/conference/iswc/2010/poster-demo-proceedings"/>
    <rdfs:label>A Graphical Evaluation Tool for Semantic Web Service Matchmaking</rdfs:label>
    <owl:sameAs rdf:resource="http://data.semanticweb.org/conference/iswc/2010/paper/501"/>

  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/iswc2010/paper/385">
    <rdfs:label>I18n of Semantic Web Applications</rdfs:label>
    <cofunc:title>I18n of Semantic Web Applications</cofunc:title>

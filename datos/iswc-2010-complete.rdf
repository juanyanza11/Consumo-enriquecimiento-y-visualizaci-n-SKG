    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/josiane-xavier-parreira"/>
    <cofunc:keyword>SPARQL query processor</cofunc:keyword>
    <rdfs:label>RDF On the Go: An RDF Storage and Query Processor for Mobile Devices</rdfs:label>
    <cofunc:keyword>mobile devices</cofunc:keyword>

  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/iswc2010/paper/274">
    <cofunc:keyword>semantic web</cofunc:keyword>
    <cofunc:title>Completeness Guarantees for Incomplete Reasoners</cofunc:title>
    <rdf:type rdf:resource="https://w3id.org/scholarlydata/ontology/conference-ontology.owl#InProceedings"/>

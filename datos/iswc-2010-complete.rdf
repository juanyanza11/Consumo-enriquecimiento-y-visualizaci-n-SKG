  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/iswc2010/paper/448/authorList/item-1">
    <cofunc:hasContent rdf:resource="https://w3id.org/scholarlydata/person/haishan-liu"/>

  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/iswc2010/paper/439">
    <cofunc:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/iswc2010/paper/439/authorList"/>

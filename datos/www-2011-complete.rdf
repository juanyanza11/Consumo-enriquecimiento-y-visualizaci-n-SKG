    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/masahiro-hamasaki"/>
    <cofunc:isPartOf rdf:resource="https://w3id.org/scholarlydata/conference/www/2011/proceedings"/>
    <rdfs:label>An Adaptive Ontology-Based Approach to Identify Correlation Between Publications</rdfs:label>

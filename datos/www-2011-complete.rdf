  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/www2011/poster/how-to-choose-combinations-in-a-join-of-search-res">
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/yaron-kanza"/>

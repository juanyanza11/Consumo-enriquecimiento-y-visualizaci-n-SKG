  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/www2011/poster/automatically-generating-labels-based-on-unified-c">
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/guichun-hua"/>
    <cofunc:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/www2011/poster/automatically-generating-labels-based-on-unified-c/authorList"/>

  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/www2011/poster/investigating-topic-models-for-social-media-user-r">
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/siva-gurumurthy"/>

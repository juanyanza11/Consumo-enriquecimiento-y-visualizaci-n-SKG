    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/cheng-te-li"/>
    <cofunc:title>Finding Influential Mediators in Social Networks</cofunc:title>
    <rdf:type rdf:resource="https://w3id.org/scholarlydata/ontology/conference-ontology.owl#InProceedings"/>

  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/www2011/paper/efficiently-evaluating-graph-constraints-in-conten">
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/bhaskar-ghosh"/>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/andrei-broder"/>

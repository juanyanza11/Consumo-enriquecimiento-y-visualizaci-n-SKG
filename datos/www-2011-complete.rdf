    <cofunc:title>How to Choose Combinations in a Join of Search Results</cofunc:title>
    <cofunc:isPartOf rdf:resource="https://w3id.org/scholarlydata/conference/www/2011/proceedings"/>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/mirit-shalem"/>
    <owl:sameAs rdf:resource="http://data.semanticweb.org/conference/www/2011/poster/how-to-choose-combinations-in-a-join-of-search-res"/>

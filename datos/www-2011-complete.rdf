  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/www2011/paper/track-globally-deliver-locally-improving-content-d">
    <rdf:type rdf:resource="https://w3id.org/scholarlydata/ontology/conference-ontology.owl#InProceedings"/>
    <cofunc:hasTopic rdf:resource="http://data.semanticweb.org/topic/performance-and-systems"/>

    <owl:sameAs rdf:resource="http://data.semanticweb.org/conference/www/2011/paper/a-case-for-query-by-image-and-text-content-searchi"/>
    <cofunc:hasTopic rdf:resource="http://dbpedia.org/resource/World_Wide_Web"/>
  </rdf:Description>

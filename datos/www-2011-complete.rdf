  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/www2011/paper/a-case-for-query-by-image-and-text-content-searchi">
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/boriz-katz"/>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/tom-yeh"/>

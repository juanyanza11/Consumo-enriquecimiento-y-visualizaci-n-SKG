  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/www2011/paper/we-know-who-you-followed-last-summer-inferring-soc">
    <cofunc:hasTopic rdf:resource="http://dbpedia.org/resource/World_Wide_Web"/>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/brian-karrer"/>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/brendan-meeder"/>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/jennifer-chayes"/>
    <owl:sameAs rdf:resource="http://data.semanticweb.org/conference/www/2011/paper/we-know-who-you-followed-last-summer-inferring-soc"/>

    <j.0:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/ewfe2012/data.semanticweb.org/workshop/ewfe/2012/paper/2/authorList"/>
    <j.0:keyword>science learning</j.0:keyword>

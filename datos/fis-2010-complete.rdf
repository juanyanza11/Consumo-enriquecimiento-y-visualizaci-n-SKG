    <rdfs:label>Access Control for RDF: Experimental Results</rdfs:label>
    <owl:sameAs rdf:resource="http://data.semanticweb.org/conference/fis/2010/paper/posters/8"/>
    <cofunc:title>Access Control for RDF: Experimental Results</cofunc:title>
    <cofunc:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/fis2010/paper/posters/8/authorList"/>
    <rdf:type rdf:resource="https://w3id.org/scholarlydata/ontology/conference-ontology.owl#InProceedings"/>
    <cofunc:hasTopic rdf:resource="http://dbpedia.org/resource/Semantic_Web"/>

    <cofunc:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/fis2010/paper/internet_of_services_things_content/4/authorList"/>
    <cofunc:hasTopic rdf:resource="http://dbpedia.org/resource/Semantic_Web"/>

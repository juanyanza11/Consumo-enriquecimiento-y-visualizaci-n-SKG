  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/ssws2008/data.semanticweb.org/workshop/ssws/2008/paper/main/2">
    <cofunc:isPartOf rdf:resource="https://w3id.org/scholarlydata/workshop/ssws/2008/proceedings"/>

  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/fews2007/data.semanticweb.org/workshop/fews/2007/papers/500">
    <cofunc:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/fews2007/data.semanticweb.org/workshop/fews/2007/papers/500/authorList"/>

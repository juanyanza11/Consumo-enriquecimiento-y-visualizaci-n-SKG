  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/ldow2008/data.semanticweb.org/workshop/LDOW/2008/paper/23">
    <cofunc:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/ldow2008/data.semanticweb.org/workshop/LDOW/2008/paper/23/authorList"/>
    <rdf:type rdf:resource="https://w3id.org/scholarlydata/ontology/conference-ontology.owl#InProceedings"/>
    <rdfs:label>Weaving SIOC into the Web of Linked Data</rdfs:label>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/uldis-bojars"/>
    <cofunc:title>Weaving SIOC into the Web of Linked Data</cofunc:title>
    <cofunc:abstract>Social media sites offer a rich source of large amounts of data. The SIOC ontology and related tools allow to express this information as linked RDF data. This paper describes approaches for making this information an integral part of the Web of Data.</cofunc:abstract>
    <cofunc:isPartOf rdf:resource="https://w3id.org/scholarlydata/workshop/LDOW/2008/proceedings"/>
    <owl:sameAs rdf:resource="http://data.semanticweb.org/workshop/LDOW/2008/paper/23"/>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/alexandre-passant"/>
  </rdf:Description>

    <cofunc:title>Provenance and Linked Data in Biological Data Webs</cofunc:title>
    <rdf:type rdf:resource="https://w3id.org/scholarlydata/ontology/conference-ontology.owl#InProceedings"/>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/david-shotton"/>

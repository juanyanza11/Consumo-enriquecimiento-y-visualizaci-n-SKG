  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/eswc2007/paper-282">
    <cofunc:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/eswc2007/paper-282/authorList"/>

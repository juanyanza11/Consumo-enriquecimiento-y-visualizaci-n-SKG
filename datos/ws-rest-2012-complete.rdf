    <rdfs:label>Functional Descriptions as the Bridge between Hypermedia APIs and the Semantic Web</rdfs:label>
    <j.0:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/ws-rest2012/data.semanticweb.org/workshop/wsrest/2012/paper/9/authorList"/>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/ruben-verborgh"/>

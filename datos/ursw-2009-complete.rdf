    <j.0:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/ursw2009/data.semanticweb.org/workshop/ursw/2009/paper/main/1/authorList"/>
    <j.0:title>Ontology Granulation Through Inductive Decision Trees</j.0:title>

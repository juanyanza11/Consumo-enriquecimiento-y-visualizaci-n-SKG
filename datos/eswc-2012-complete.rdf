    <owl:sameAs rdf:resource="http://data.semanticweb.org/conference/eswc/2012/paper/research/218"/>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/dragan-gasevic"/>
    <rdfs:label>Voting Theory for Concept Detection</rdfs:label>
    <cofunc:keyword>concepts</cofunc:keyword>

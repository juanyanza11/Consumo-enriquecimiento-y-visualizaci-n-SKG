  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/eswc2012/paper/demonstation/323">
    <rdfs:label>Does it fit? KOS evaluation using the ICE-Map Visualization.</rdfs:label>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/kai-eckert"/>
    <cofunc:keyword>Visualisation</cofunc:keyword>

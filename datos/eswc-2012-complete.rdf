  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/eswc2012/paper/poster/314">
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/eva-winer"/>
    <rdfs:label>APA VIVO: A Semantic Framework for Scholarly Identity and Trusted Attribute Exchange</rdfs:label>
    <cofunc:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/eswc2012/paper/poster/314/authorList"/>

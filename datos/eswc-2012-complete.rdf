  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/eswc2012/paper/poster/302">
    <cofunc:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/eswc2012/paper/poster/302/authorList"/>

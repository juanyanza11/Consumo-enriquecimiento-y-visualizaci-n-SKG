  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/terra2009/data.semanticweb.org/workshop/terra_cognita/2009/paper/main/7">
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/anthony-g-cohn"/>
    <j.0:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/terra2009/data.semanticweb.org/workshop/terra_cognita/2009/paper/main/7/authorList"/>

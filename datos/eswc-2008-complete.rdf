  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/eswc2008/paper/11">
    <owl:sameAs rdf:resource="http://data.semanticweb.org/conference/eswc/2008/paper/11"/>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/manuel-ramos-cabrer"/>

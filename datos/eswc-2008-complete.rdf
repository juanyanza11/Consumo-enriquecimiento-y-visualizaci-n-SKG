    <owl:sameAs rdf:resource="http://data.semanticweb.org/conference/eswc/2008/paper/107"/>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/vassilis-spiliopoulos"/>
    <cofunc:keyword>supervised machine learning</cofunc:keyword>
    <cofunc:hasTopic rdf:resource="http://dbpedia.org/resource/Ontology_alignment"/>
    <cofunc:isPartOf rdf:resource="https://w3id.org/scholarlydata/conference/eswc/2008/proceedings"/>
    <rdfs:label>CSR: Discovering Subsumption Relations for the Alignment of Ontologies</rdfs:label>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/george-vouros"/>

    <cofunc:title>A Functional Semantic Web Architecture</cofunc:title>
    <rdf:type rdf:resource="https://w3id.org/scholarlydata/ontology/conference-ontology.owl#InProceedings"/>
    <owl:sameAs rdf:resource="http://data.semanticweb.org/conference/eswc/2008/paper/152"/>

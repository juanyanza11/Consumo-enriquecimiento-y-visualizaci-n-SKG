    <j.0:keyword>taxonomy enrichment</j.0:keyword>
    <j.0:keyword>document category histograms (DCH)</j.0:keyword>
    <j.0:keyword>extended growing hierarchical self-organizing maps (Enrich-GHSOM)</j.0:keyword>

  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/www2008/paper/497">
    <cofunc:keyword>semantic web</cofunc:keyword>
    <cofunc:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/www2008/paper/497/authorList"/>

    <owl:sameAs rdf:resource="http://data.semanticweb.org/conference/www/2008/paper/138"/>
    <cofunc:keyword>Sponsored Search</cofunc:keyword>
    <rdfs:label>Dynamic Cost-Per-Action Mechanisms and Applications to Online Advertising</rdfs:label>
    <cofunc:hasTopic rdf:resource="http://dbpedia.org/resource/World_Wide_Web"/>

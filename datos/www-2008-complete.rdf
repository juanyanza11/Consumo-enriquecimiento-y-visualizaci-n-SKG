    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/stijn-vansummeren"/>
    <cofunc:keyword>schema inference</cofunc:keyword>
    <cofunc:title>Learning Deterministic Regular Expressions for the Inference of Schemas from XML Data</cofunc:title>

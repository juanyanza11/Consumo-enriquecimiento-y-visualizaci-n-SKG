    <cofunc:hasTopic rdf:resource="http://dbpedia.org/resource/World_Wide_Web"/>
    <cofunc:title>Scaling RDF with Time</cofunc:title>
    <cofunc:keyword>Temporal RDF</cofunc:keyword>
  </rdf:Description>

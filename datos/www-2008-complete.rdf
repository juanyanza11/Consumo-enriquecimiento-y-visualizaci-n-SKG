  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/www2008/paper/765">
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/wenyuan-dai"/>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/qiang-yang"/>

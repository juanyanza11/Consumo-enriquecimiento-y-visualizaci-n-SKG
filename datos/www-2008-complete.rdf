    <rdf:type rdf:resource="https://w3id.org/scholarlydata/ontology/conference-ontology.owl#InProceedings"/>
    <rdfs:label>Contextual Advertising by Combining Relevance with Click Feedback</rdfs:label>
    <cofunc:keyword>Modeling</cofunc:keyword>

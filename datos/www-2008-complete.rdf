    <cofunc:keyword>Topic Distillation</cofunc:keyword>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/wen-ying-xiong"/>
    <cofunc:keyword>Relational Ranking SVM</cofunc:keyword>

    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/gert-lanckriet"/>
    <j.0:keyword>music recommendation</j.0:keyword>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/brian-mcfee"/>

  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/admire2012/data.semanticweb.org/workshop/admire/2012/paper/2">
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/marcos-aurelio-domingues"/>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/jose-paulo-leal"/>

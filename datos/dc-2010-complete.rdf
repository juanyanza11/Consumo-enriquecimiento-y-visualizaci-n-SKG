  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/dc-2010/paper/main/A-1009">
    <cofunc:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/dc-2010/paper/main/A-1009/authorList"/>

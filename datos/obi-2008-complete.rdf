    <j.0:previous rdf:resource="https://w3id.org/scholarlydata/inproceedings/obi2008/data.semanticweb.org/workshop/obi/2008/paper/main/8/authorList/item-3"/>
    <j.0:hasContent rdf:resource="https://w3id.org/scholarlydata/person/alex-kass"/>

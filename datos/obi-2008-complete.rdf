  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/obi2008/data.semanticweb.org/workshop/obi/2008/paper/main/9">
    <j.0:keyword>Information Integration</j.0:keyword>
    <j.0:keyword>Conceptual Query Design</j.0:keyword>
    <j.0:keyword>Business Intelligence</j.0:keyword>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/michael-spahn"/>
    <owl:sameAs rdf:resource="http://data.semanticweb.org/workshop/obi/2008/paper/main/9"/>

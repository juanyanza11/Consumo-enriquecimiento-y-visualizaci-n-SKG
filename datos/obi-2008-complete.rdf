    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/hamish-cunningham"/>
    <j.0:keyword>Ontology-based Information Extaction</j.0:keyword>
    <j.0:keyword>Identity Resolution</j.0:keyword>
    <j.0:keyword>Co-reference</j.0:keyword>

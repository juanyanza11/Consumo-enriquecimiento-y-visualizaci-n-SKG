    <rdf:type rdf:resource="https://w3id.org/scholarlydata/ontology/conference-ontology.owl#InProceedings"/>
    <cofunc:keyword>Language Technology and Interoperability</cofunc:keyword>
    <cofunc:keyword>Multilingual Web</cofunc:keyword>
    <cofunc:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/msw2011/data.semanticweb.org/workshop/msw/2011/paper/8/authorList"/>

  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/esoe2007/data.semanticweb.org/workshop/esoe/2007/papers/571">
    <cofunc:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/esoe2007/data.semanticweb.org/workshop/esoe/2007/papers/571/authorList"/>

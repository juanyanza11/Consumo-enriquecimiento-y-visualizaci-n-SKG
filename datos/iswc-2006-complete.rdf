  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/iswc2006/paper-9">
    <j.0:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/iswc2006/paper-9/authorList"/>

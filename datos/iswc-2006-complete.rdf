  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/iswc2006/paper-41">
    <j.0:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/iswc2006/paper-41/authorList"/>

  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/om2008/data.semanticweb.org/workshop/om/2008/paper/main/12">
    <rdf:type rdf:resource="https://w3id.org/scholarlydata/ontology/conference-ontology.owl#InProceedings"/>

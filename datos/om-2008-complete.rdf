  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/om2008/data.semanticweb.org/workshop/om/2008/paper/main/28">
    <j.0:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/om2008/data.semanticweb.org/workshop/om/2008/paper/main/28/authorList"/>

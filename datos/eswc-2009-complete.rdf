  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/eswc2009/paper/205">
    <cofunc:hasTopic rdf:resource="http://dbpedia.org/resource/Inference"/>
    <cofunc:hasTopic rdf:resource="http://dbpedia.org/resource/Formal_languages"/>

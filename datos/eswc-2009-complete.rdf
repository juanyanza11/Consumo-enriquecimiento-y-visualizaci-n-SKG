    <cofunc:title>Leveraging Semantic Web Service Descriptions for Validation by Automated Functional Testing</cofunc:title>
    <cofunc:hasTopic rdf:resource="http://dbpedia.org/resource/Rule_of_inference"/>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/dimitrios-kourtesis"/>
    <cofunc:keyword>Service Validation</cofunc:keyword>
    <cofunc:hasTopic rdf:resource="http://dbpedia.org/resource/Semantic_Web_Services"/>

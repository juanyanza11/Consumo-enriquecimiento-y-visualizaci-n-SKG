  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/eswc2009/paper/125">
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/ingo-zinnikus"/>
    <cofunc:hasTopic rdf:resource="http://dbpedia.org/resource/Web_service"/>

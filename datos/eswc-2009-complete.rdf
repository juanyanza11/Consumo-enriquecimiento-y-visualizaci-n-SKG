  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/eswc2009/paper/27">
    <cofunc:hasTopic rdf:resource="http://dbpedia.org/resource/Data_mining"/>
    <cofunc:hasTopic rdf:resource="http://dbpedia.org/resource/Query"/>

    <cofunc:hasTopic rdf:resource="http://dbpedia.org/resource/Test"/>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/sebastian-boehm"/>
    <cofunc:keyword>real-world experiences</cofunc:keyword>

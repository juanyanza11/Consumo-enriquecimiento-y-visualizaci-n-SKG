    <rdfs:label>Representing, Querying and Transforming Social Networks with RDF/SPARQL</rdfs:label>
    <cofunc:keyword>Social Networks</cofunc:keyword>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/claudio-gutierrez"/>
    <cofunc:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/eswc2009/paper/232/authorList"/>

    <cofunc:hasTopic rdf:resource="http://dbpedia.org/resource/Database_management_system"/>
    <cofunc:isPartOf rdf:resource="https://w3id.org/scholarlydata/conference/eswc/2009/proceedings"/>
    <rdfs:label>Tempus Fugit - Towards an Ontology Update Language</rdfs:label>
    <cofunc:keyword>Ontology Update Framework</cofunc:keyword>
    <cofunc:title>Tempus Fugit - Towards an Ontology Update Language</cofunc:title>

  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/ruleml2011-europe/paper/62">
    <cofunc:keyword>rules</cofunc:keyword>
    <cofunc:keyword>agents</cofunc:keyword>
    <cofunc:keyword>track paper on norms and agents</cofunc:keyword>
    <cofunc:title>Rules, Agents and Norms: Guidelines for Rule-based Normative Multi-agent Systems</cofunc:title>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/antonino-rotolo"/>
    <cofunc:keyword>norms</cofunc:keyword>
    <cofunc:isPartOf rdf:resource="https://w3id.org/scholarlydata/conference/ruleml-eu/2011/proceedings"/>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/leon-van-der-torre"/>

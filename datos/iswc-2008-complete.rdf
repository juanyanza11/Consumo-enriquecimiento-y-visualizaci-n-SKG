    <cofunc:keyword>cultural heritage</cofunc:keyword>
    <cofunc:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/iswc2008/paper/doctoral_consortium/18/authorList"/>
    <rdf:type rdf:resource="https://w3id.org/scholarlydata/ontology/conference-ontology.owl#InProceedings"/>

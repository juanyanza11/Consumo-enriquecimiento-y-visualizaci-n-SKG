    <cofunc:title>Semantic Enrichment of Folksonomy Tagspaces</cofunc:title>
    <cofunc:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/iswc2008/paper/doctoral_consortium/31/authorList"/>

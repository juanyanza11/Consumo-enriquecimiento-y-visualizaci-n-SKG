    <cofunc:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/iswc2008/paper/poster_demo/1/authorList"/>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/peter-beck"/>
    <cofunc:keyword>Query Evaluation</cofunc:keyword>
    <owl:sameAs rdf:resource="http://data.semanticweb.org/conference/iswc/2008/paper/poster_demo/1"/>
    <rdfs:label>Indexing Social Semantic Data</rdfs:label>
  </rdf:Description>

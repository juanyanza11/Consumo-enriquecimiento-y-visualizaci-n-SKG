  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/iswc2008/paper/research/178">
    <cofunc:keyword>SPARQL</cofunc:keyword>
    <cofunc:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/iswc2008/paper/research/178/authorList"/>
    <cofunc:isPartOf rdf:resource="https://w3id.org/scholarlydata/conference/iswc/2008/proceedings"/>

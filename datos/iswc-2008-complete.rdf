    <cofunc:title>SemTree: Ontology-Based Decision Tree Algorithm for Recommender Systems</cofunc:title>
    <cofunc:keyword>Ontology-Based Decision Tree</cofunc:keyword>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/harald-gall"/>
    <cofunc:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/iswc2008/paper/poster_demo/87/authorList"/>

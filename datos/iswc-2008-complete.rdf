  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/iswc2008/paper/research/229">
    <cofunc:keyword>Classification</cofunc:keyword>
    <cofunc:keyword>Similarity</cofunc:keyword>
    <cofunc:hasTopic rdf:resource="http://dbpedia.org/resource/Semantic_Web"/>

    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/georg-lausen"/>
    <cofunc:keyword>RDF Data Management</cofunc:keyword>
    <owl:sameAs rdf:resource="http://data.semanticweb.org/conference/iswc/2008/paper/research/178"/>
    <cofunc:hasTopic rdf:resource="http://dbpedia.org/resource/Semantic_Web"/>

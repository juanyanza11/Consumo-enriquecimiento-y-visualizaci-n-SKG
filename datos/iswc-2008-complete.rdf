    <cofunc:hasTopic rdf:resource="http://dbpedia.org/resource/Semantic_Web"/>
    <cofunc:isPartOf rdf:resource="https://w3id.org/scholarlydata/conference/iswc/2008/proceedings"/>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/gong-cheng"/>
    <cofunc:keyword>complex network analysis</cofunc:keyword>
    <cofunc:title>Term Dependence on the Semantic Web</cofunc:title>

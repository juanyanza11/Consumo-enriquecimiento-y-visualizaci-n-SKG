    <rdf:type rdf:resource="https://w3id.org/scholarlydata/ontology/conference-ontology.owl#InProceedings"/>
    <cofunc:title>Thesaurus-based Search in Large Heterogeneous Collections</cofunc:title>
    <cofunc:keyword>semantic search</cofunc:keyword>
    <owl:sameAs rdf:resource="http://data.semanticweb.org/conference/iswc/2008/paper/in_use/12"/>

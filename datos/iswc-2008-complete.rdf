    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/carlo-allocca"/>
    <rdf:type rdf:resource="https://w3id.org/scholarlydata/ontology/conference-ontology.owl#InProceedings"/>
    <owl:sameAs rdf:resource="http://data.semanticweb.org/conference/iswc/2008/paper/poster_demo/38"/>
    <cofunc:keyword>Semantic Equivalence</cofunc:keyword>

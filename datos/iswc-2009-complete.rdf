  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/iswc2009/paper/inuse/183">
    <owl:sameAs rdf:resource="http://data.semanticweb.org/conference/iswc/2009/paper/inuse/183"/>

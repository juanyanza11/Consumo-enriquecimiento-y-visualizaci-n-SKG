    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/kerstin-denecke"/>
    <owl:sameAs rdf:resource="http://data.semanticweb.org/conference/iswc/2009/paper/poster_demo/107"/>
    <cofunc:isPartOf rdf:resource="https://w3id.org/scholarlydata/conference/iswc/2009/proceedings"/>

    <cofunc:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/iswc2009/paper/research/524/authorList"/>
    <rdfs:label>Exploiting User Feedback to Improve Semantic Web Service Discovery</rdfs:label>
    <cofunc:hasTopic rdf:resource="http://dbpedia.org/resource/Semantic_Web"/>

  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/iswc2009/paper/research/240">
    <rdf:type rdf:resource="https://w3id.org/scholarlydata/ontology/conference-ontology.owl#InProceedings"/>
    <cofunc:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/iswc2009/paper/research/240/authorList"/>

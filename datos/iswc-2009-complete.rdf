  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/iswc2009/paper/poster_demo/118">
    <cofunc:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/iswc2009/paper/poster_demo/118/authorList"/>
    <cofunc:hasTopic rdf:resource="http://dbpedia.org/resource/Semantic_Web"/>

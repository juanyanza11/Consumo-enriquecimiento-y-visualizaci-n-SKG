  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/iswc2009/paper/research/125">
    <cofunc:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/iswc2009/paper/research/125/authorList"/>
    <cofunc:isPartOf rdf:resource="https://w3id.org/scholarlydata/conference/iswc/2009/proceedings"/>

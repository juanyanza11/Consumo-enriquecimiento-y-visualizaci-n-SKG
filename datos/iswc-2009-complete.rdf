    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/daniel-miranker"/>
    <rdf:type rdf:resource="https://w3id.org/scholarlydata/ontology/conference-ontology.owl#InProceedings"/>
    <cofunc:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/iswc2009/paper/poster_demo/147/authorList"/>

    <cofunc:isPartOf rdf:resource="https://w3id.org/scholarlydata/conference/iswc/2009/proceedings"/>
    <owl:sameAs rdf:resource="http://data.semanticweb.org/conference/iswc/2009/paper/research/484"/>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/paul-doran"/>

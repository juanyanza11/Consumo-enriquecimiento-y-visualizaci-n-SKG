    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/renata-dividino"/>
    <cofunc:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/ekaw2012/paper/research/37/authorList"/>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/matthias-thimm"/>
    <owl:sameAs rdf:resource="http://data.semanticweb.org/conference/ekaw/2012/paper/research/37"/>
    <cofunc:title>Ranking RDF with Provenance via Preference Aggregation</cofunc:title>
    <rdf:type rdf:resource="https://w3id.org/scholarlydata/ontology/conference-ontology.owl#InProceedings"/>
  </rdf:Description>

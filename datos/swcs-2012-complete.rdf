  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/swcs2012/data.semanticweb.org/workshop/swcs/2012/paper/5">
    <rdf:type rdf:resource="https://w3id.org/scholarlydata/ontology/conference-ontology.owl#InProceedings"/>

  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/iswc2007+aswc2007/tracks/research/papers/309">
    <cofunc:hasTopic rdf:resource="http://dbpedia.org/resource/Semantic_Web"/>

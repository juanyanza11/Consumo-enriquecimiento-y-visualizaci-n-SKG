  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/iswc2007+aswc2007/tracks/in-use/papers/837">
    <rdfs:label>Ontology-based Information Extraction for Business Intelligence</rdfs:label>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/adam-funk"/>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/kalina-bontcheva"/>
    <cofunc:hasTopic rdf:resource="http://dbpedia.org/resource/Finance"/>

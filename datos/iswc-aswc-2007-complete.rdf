  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/iswc2007+aswc2007/tracks/posters-demos/papers/407">
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/michael-waltl"/>

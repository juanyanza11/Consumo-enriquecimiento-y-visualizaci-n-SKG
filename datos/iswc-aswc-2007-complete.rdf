  <rdf:Description rdf:about="https://w3id.org/scholarlydata/person/yoonyoung-nam">
    <cofunc:hasAffiliation rdf:resource="https://w3id.org/scholarlydata/affiliation-during-event/iswc2007+aswc2007-korea-advanced-institute-of-science-and-technology-yoonyoung-nam"/>

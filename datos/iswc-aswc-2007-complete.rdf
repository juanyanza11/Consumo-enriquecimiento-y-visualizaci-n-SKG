    <owl:sameAs rdf:resource="http://data.semanticweb.org/conference/iswc-aswc/2007/tracks/in-use/papers/729"/>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/sylvie-despres"/>
  </rdf:Description>

    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/tudor-groza"/>
    <cofunc:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/iswc2007+aswc2007/tracks/research/papers/197/authorList"/>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/diana-trif"/>

  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/iswc2007+aswc2007/tracks/doctoral-consortium/papers/920">
    <cofunc:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/iswc2007+aswc2007/tracks/doctoral-consortium/papers/920/authorList"/>

    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/claudio-baldassarre"/>
    <cofunc:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/iswc2007+aswc2007/tracks/posters-demos/papers/366/authorList"/>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/enrico-motta"/>

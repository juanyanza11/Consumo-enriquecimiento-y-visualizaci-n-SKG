    <owl:sameAs rdf:resource="http://data.semanticweb.org/conference/iswc-aswc/2007/tracks/research/papers/435"/>
    <cofunc:hasTopic rdf:resource="http://dbpedia.org/resource/Formal_language"/>
    <rdfs:label>Using Tableau to Decide Expressive Description Logics with Role Negation</rdfs:label>
    <cofunc:title>Using Tableau to Decide Expressive Description Logics with Role Negation</cofunc:title>

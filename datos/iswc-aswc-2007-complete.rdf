  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/iswc2007+aswc2007/tracks/posters-demos/papers/386">
    <rdfs:label>Extending Content Management Systems with Ontological Annotation Capabilities</rdfs:label>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/eero-hyvoenen"/>
    <cofunc:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/iswc2007+aswc2007/tracks/posters-demos/papers/386/authorList"/>

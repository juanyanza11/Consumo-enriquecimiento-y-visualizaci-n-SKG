    <cofunc:hasTopic rdf:resource="http://dbpedia.org/resource/Evaluation"/>
    <cofunc:hasTopic rdf:resource="http://dbpedia.org/resource/Semantic_Web"/>
    <cofunc:hasTopic rdf:resource="http://dbpedia.org/resource/Natural_language_processing"/>
    <cofunc:isPartOf rdf:resource="https://w3id.org/scholarlydata/conference/iswc-aswc/2007/proceedings"/>
    <rdfs:label>How Useful are Natural Language Interfaces to the Semantic Web for Casual End-users?</rdfs:label>

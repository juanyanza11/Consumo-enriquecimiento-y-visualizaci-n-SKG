    <owl:sameAs rdf:resource="http://data.semanticweb.org/conference/iswc-aswc/2007/tracks/posters-demos/papers/371"/>
    <rdf:type rdf:resource="https://w3id.org/scholarlydata/ontology/conference-ontology.owl#InProceedings"/>

  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/iswc2007+aswc2007/tracks/research/papers/267">
    <cofunc:hasTopic rdf:resource="http://dbpedia.org/resource/Human-computer_interaction"/>
    <cofunc:hasTopic rdf:resource="http://dbpedia.org/resource/Semantic_Web"/>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/evren-sirin"/>
    <cofunc:isPartOf rdf:resource="https://w3id.org/scholarlydata/conference/iswc-aswc/2007/proceedings"/>

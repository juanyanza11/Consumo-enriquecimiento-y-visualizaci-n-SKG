    <j.0:title>Geospatial Semantic Context Capturing for Picture Annotation, Retrieval and Visualization in SPIM</j.0:title>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/takuya-imai"/>
    <rdfs:label>Geospatial Semantic Context Capturing for Picture Annotation, Retrieval and Visualization in SPIM</rdfs:label>
    <j.0:keyword>spatial</j.0:keyword>
    <j.0:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/terracognita2008/data.semanticweb.org/workshop/terra_cognita/2008/paper/main/8/authorList"/>

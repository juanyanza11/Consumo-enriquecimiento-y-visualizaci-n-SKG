    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/chun-chen"/>
    <j.0:keyword>recommendation systems</j.0:keyword>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/can-wang"/>
    <rdfs:label>Document Recommendation in Social Tagging Services</rdfs:label>

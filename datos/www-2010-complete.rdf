  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/www2010/paper/main/845">
    <j.0:title>Exploiting Query Reformulations for Web Search Result Diversification</j.0:title>
    <j.0:keyword>ranking</j.0:keyword>
    <j.0:isPartOf rdf:resource="https://w3id.org/scholarlydata/conference/www/2010/proceedings"/>

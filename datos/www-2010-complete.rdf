  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/www2010/paper/main/578">
    <j.0:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/www2010/paper/main/578/authorList"/>

    <rdfs:label>A comparison of visual and textual page previews in judging the helpfulness of web pages</rdfs:label>
    <j.0:keyword>evaluation of web interactions</j.0:keyword>
    <owl:sameAs rdf:resource="http://data.semanticweb.org/conference/www/2010/paper/main/897"/>

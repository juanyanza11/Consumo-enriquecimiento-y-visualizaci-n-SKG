  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/www2007/paper/main/692">
    <j.0:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/www2007/paper/main/692/authorList"/>

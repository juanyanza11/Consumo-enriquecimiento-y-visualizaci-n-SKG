  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/www2007/paper/main/435">
    <j.0:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/www2007/paper/main/435/authorList"/>
